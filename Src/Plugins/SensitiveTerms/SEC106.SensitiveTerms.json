{
  "ValidatorsAssemblyName": "SensitiveTerms.dll",
  "SharedStringsFileName": "SensitiveTerms.SharedStrings.txt",
  "Definitions": [
    {
      "Id": "SEC106",
      "Name": "DoNotUseSensitiveTerms",
      "Level": "Warning",
      "FileNameAllowRegex": "$SourceFiles",
      "Description": "Do not use sensitive terms in all files.",
      "Message": "'{0:scanTarget}' contains sensitive term '{1:term}'.",
      "MatchExpressions": [
        {
          "Id": "SEC106/001",
          "Name": "DoNotUseSensitiveTerms/Edge",
          "ContentsRegex": "$SEC106/001.Term.Edge",
          "MessageArguments": { "term": "edge" }
        }
      ]
    }
  ]
}
