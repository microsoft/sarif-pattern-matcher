{
  "Definitions": [
    {
      "Name": "DoNotUseSensitiveFile",
      "Id": "SEN1001",
      "Level": "Error",
      "DefaultNameRegex": "(?i)\\.(pem|pkcs12|p12|pfx|asc|ovpn|rdp|mdf|sdf|sqlite|sqlite3|bek|tpm|fve|jks|psafe3|agilekeychain|kwallet|tblk|ppk)$",
      "Description": "Developers should not check-in sensitive files.",
      "Message": "'{0:scanTarget}' is a sensitive file.",
      "MatchExpressions": [
        {
          "SubId": "SensitiveFile",
          "ContentsRegex": "*",
          "MessageArguments": { "secureApi": "Sensitive File" }
        }
      ]
    }
  ]
}