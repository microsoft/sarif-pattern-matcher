{
  "ValidatorsAssemblyName": "SalAnalyzer.dll",
  "SharedStringsFileName": "SalAnalyzer.SharedStrings.txt",
  "Definitions": [
    {
      "Id": "SEC105",
      "Name": "CorrectUseOfSal",
      "Level": "Warning",
      "FileNameAllowRegex": "$CSourceFiles",
      "Description": "Use SAL correctly.",
      "Message": "'{0:scanTarget}' contains incorrect or out-dated use of SAL: {1:validatorMessage}.",
      "MatchExpressions": [
        {
          "Id": "SEC105/001",
          "Name": "CorrectUseOfSal/Allocator",
          "ContentsRegex": "$SEC105/001.Allocator",
          "Fixes": {
            "testKey": {
              "Description": "Delete Allocator",
              "Find": "__allocator",
              "ReplaceWith": ""
            }
          }
        },
        {
          "Id": "SEC105/002",
          "Name": "CorrectUseOfSal/AnalysisAssume",
          "ContentsRegex": "$SEC105/002.AnalysisAssume",
          "Fixes": {
            "testKey": {
              "Description": "Replace __analysis_assume with _Analysis_assume_",
              "Find": "__analysis_assume",
              "ReplaceWith": "_Analysis_assume_"
            }
          }
        }
      ]
    }
  ]
}
