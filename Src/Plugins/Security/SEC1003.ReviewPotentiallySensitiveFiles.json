      "MatchExpressions": [
        {
          "SubId": "1PasswordFile",
          "NameAllowList": "(?i)\\.agilekeychain$",
          "ContentsRegex": ".",
          "MessageArguments": { "fileKind": "a 1Password manager database file" }
