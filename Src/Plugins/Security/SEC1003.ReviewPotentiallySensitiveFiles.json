{
  "Definitions": [
    {
      "Name": "ReviewPotentiallySensitiveFiles",
      "Id": "SEC1003",
      "Level": "Error",
      "FileNameAllowRegex": "(?i)\\.(agilekeychain|asc|bek|fve|jks|kwallet|mdf|ovpn|p12|pem|pfx|pkcs12|ppk|psafe3|rdp|sdf|sqlite|sqlite3|tblk|tpm)$",
